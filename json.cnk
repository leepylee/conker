
struct StorageItem
  str name
  num type
  num boxNo
end

struct Storage
  StorageItem[] storageItems
end

func main()
  Storage storage
  num index
  if exists('storage.dat')
    loaddata storage, 'storage.dat'
  else
    StorageItem si = { name = 'Camp bed', type = 1, boxNo = 42 }  
    add storage.storageItems, si
    savedata storage, 'storage.dat'
  end
  print storage
end
