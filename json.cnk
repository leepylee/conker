
struct Storage
  str name
  num type
  num boxNo
end

func main()
  Storage[] storages
  if exists('storage.dat')
    loaddata storages, 'storage.dat'
  else
    StorageItem si = { name = 'Camp bed', type = 1, boxNo = 42 }  
    add storages, si
    savedata storages, 'storage.dat'
  end
  print storages
end
