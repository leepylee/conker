
func main()
  str[] parts
  add parts, 'K' + RED
  add parts, 'P0 250'
  add parts, 'L500 0 550 100'
  add parts, 'R110 0 150 300'
  add parts, 'U200 0 280 500 20 20'
  add parts, 'C0 100 100'
  add parts, 'A450 0 500 300 35 100 1'
  add parts, 'O300 0 430 300'
  add parts, 'Y3 600 0 700 300 600 620'
  str elements = join(parts, ' ')
  shape s, elements
  num x
end

func draw()
  style FILL
  strokewidth 10
  drawshape s, x, MIDY
  x += 5
end 
